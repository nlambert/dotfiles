rofi.color-enabled: true
rofi.color-window: base00, base00, base01
rofi.color-normal: base00, #657b83, base00, base00, base0C 
rofi.color-active: base00, #657b83, base00, base00, base0C 
rofi.color-urgent: base00, #657b83, base00, base00, base0C 

rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.font: NotoMono Nerd Font Mono 18
rofi.columns: 2
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
rofi.fullscreen: true
rofi.padding: 700
rofi.bw: 0
